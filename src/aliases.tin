#CLASS {aliases} {kill};
#CLASS {aliases} {open};

#ALIAS {save vars} {
    #CLASS variables write {src/variables.tin};
    #format {tmp} {%l} {${CURRENT_CHAR}};
    #CLASS {$tmp} {write} {characters/$tmp.tin};
};

#ALIAS {rst} {
    #IF {"%1" != ""} {#READ src/%1.tin};
    #ELSE {#READ config.tin;};
};

#ALIAS {clc} {
    clear;
    #DELAY 1 {#split 2 2; __update_prompt__};
};

#NOP ==[Specific character aliases]==
#ALIAS {aldr mvs} {
    #class {aldrich} {open};
    #IF {"%1" == ""} {
        #IF {"${mimic_vs}" == "1"} {#var {mimic_vs} {0}};
        #ELSE {#var {mimic_vs} {1}};
    };
    #ELSE {#var {mimic_vs_target} {%1}};
    #SHOWME \e[91m===Mimic status: $mimic_vs, target: $mimic_vs_target!===\e[0m;
    #class {aldrich} {close};
};
#ALIAS {aldr me} {
    #class {aldrich} {open};
    #IF {"%1" == ""} {
        #IF {"${mimic_endure}" == "1"} {#var {mimic_endure} {0}};
        #ELSE {#var {mimic_endure} {1}};
