    #UNACTION {Your feet feel swift.};
    scoreaf;
    #DELAY {2} {
        #ACTION {Your feet feel swift.} {
            #SHOWME \e[92mFleet UP!;
            _update_noti_ \e[92mFleet UP!\e[0m;
            _update_buff_status_ fleet on;
        };
    };
};
