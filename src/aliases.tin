#NOP ==[World interaction]==
#ALIAS {op} {open door}
#ALIAS {dk} {drop key}
#ALIAS {ud} {unlock door}
#ALIAS {eh} {enter hole}
#ALIAS {et} {enter tunnel}
#ALIAS {ep} {enter portal}
#ALIAS {tk} {take all.key all.corpse}
#ALIAS {cd} {close door};
#ALIAS {ud%w} {unlock door %1; open door %1; %1};
#ALIAS {tkd} {take key; ud%1};
#ALIAS {ent} {
    #IF {"${CURRENT_CHAR}" == "Shuu"} {o fol ent %1;};
    gs ent %1; enter %1;
};
#ALIAS {coffin} {
    get coffin haver;
    drop coffin;
    gsay ent coffin;
    #DELAY 3 {
        take coffin;
        home 1;
        weave 'word of recall';
        put coffin haver;
    };
};
#ALIAS {sigil} {
    get sigil haver;
    drop sigil;
    gsay ent sigil;
    #DELAY 3 {
        take sigil;
        home 2;
        weave 'word of recall';
        put sigil haver;
    };
};

#NOP ==[NIGERUNDAYO!!!]==
#ALIAS {f} {flee}

#NOP ==[Fighting]==
#ALIAS {k} {
    #IF {"${CURRENT_CHAR}" == "Shuu"} {order fol kill $target};
    #ELSE {
        #IF {"%1" != ""} {kill %1;};
        #ELSE {kill $target;};
    };
};
#ALIAS {a} {
    #IF {"${CURRENT_CHAR}" == "Shuu"} {order fol kill $target};
    #ELSE {assist};
} {5};
#ALIAS {a %w} {
    #VAR {assist} {%1};
    #SHOWME {\e[91m===== CURRENT ASSITING: $assist =====};
} {4};
#ALIAS {t} {
    #IF {"%1" == ""} {#ECHO {\e[91m===== DID NOT CHANGE TARGET! =====};};
    #ELSE {
        #VAR {target} {%1};
        #SHOWME {\e[91m===== CURRENT TARGET: $target =====};
    };
};

